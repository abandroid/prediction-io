<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="theme-color" content="#224470">
    <title>{% block title %}PredictionIO Recommendation Engine{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ preload(asset('/build/base.css', 'endroid_prediction_io')) }}">
    {% endblock %}
</head>
<body>
{% block body %}
    <div class="container-fluid">
        <div class="row">
            <main class="col-sm-12 col-md-12 pt-3" role="main">
                {% block content %}

                {% endblock %}
            </main>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ preload(asset('/build/manifest.js', 'endroid_prediction_io')) }}"></script>
    <script src="{{ preload(asset('/build/base.js', 'endroid_prediction_io')) }}"></script>
{% endblock %}
</body>
</html>
