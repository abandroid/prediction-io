{
    "name": "endroid/prediction-io",
    "description": "Endroid PredictionIO Client",
    "keywords": ["endroid", "prediction", "io", "predictionio"],
    "homepage": "https://github.com/endroid/PredictionIO",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "predictionio/predictionio": "~0.8"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.9",
        "phpunit/phpunit": "~4.7",
        "symfony/var-dumper": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\PredictionIO\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
