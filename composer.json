{
    "name": "endroid/prediction-io",
    "description": "Endroid PredictionIO Client",
    "keywords": ["endroid", "prediction", "io", "predictionio", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/PredictionIO",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "predictionio/predictionio": "^0.9",
        "endroid/asset": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\PredictionIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\PredictionIO\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
